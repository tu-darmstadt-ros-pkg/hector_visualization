#!/usr/bin/env python

import sys
import roslib; roslib.load_manifest('hector_rqt_joint_control')
import rospy
from hector_rqt_joint_control.joint_control_qt import JointControlDialog

from PySide.QtCore import * 
from PySide.QtGui import *

if __name__ == '__main__':
    # Initialize the node
    rospy.init_node('hector_rqt_joint_control')

    # Initialize Qt
    app = QApplication(sys.argv)

    gui = JointControlDialog()
    gui.show()

    app.exec_()